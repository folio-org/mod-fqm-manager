//        type: 'LEFT JOIN',
//        joinTo: 'holdings',
//        condition: "(:that.jsonb ->> 'permanentLocationId')::uuid = :this.id",
//      },
//    },
    {
      alias: 'bill_to',
      type: 'entity-type',
      id: 'abb28bac-e090-48c1-a48b-3fe805638041', // simple_config_data
      join: {
        type: 'LEFT JOIN',
        joinTo: 'purchase_order.po',
        condition: "jsonb_extract_path_text(:that.jsonb, 'billTo')::uuid = :this.id",
      },
    },
    {
      alias: 'ship_to',
      type: 'entity-type',
      id: 'abb28bac-e090-48c1-a48b-3fe805638041', // simple_config_data
      join: {
        type: 'LEFT JOIN',
        joinTo: 'purchase_order.po',
        condition: "jsonb_extract_path_text(:that.jsonb, 'shipTo')::uuid = :this.id",
      },
    },
    {
      alias: 'created_by_user',
      type: 'entity-type',
      id: 'f2615ea6-450b-425d-804d-6a495afd9308', // simple_FOLIO_user
      join: {
        type: 'LEFT JOIN',
        joinTo: 'purchase_order.po',
        condition: "jsonb_extract_path_text(:that.jsonb, 'metadata', 'createdByUserId')::uuid = :this.id",
      },
    },
    {
      alias: 'organization',
      type: 'entity-type',
      id: 'b5ffa2e9-8080-471a-8003-a8c5a1274503', // simple_organization
      join: {
        type: 'left join',
        joinTo: 'purchase_order.po',
        condition: "(:that.jsonb ->> 'vendor')::uuid = :this.id",
      },
    },
  ],
  columns: [
    {
      name: 'bill_to_name',
      sourceAlias: 'bill_to.config',
      dataType: {
        dataType: 'stringType',
      },
      queryable: true,
      visibleByDefault: true,
      valueGetter: "json_extract_path_text((:sourceAlias.jsonb->>'value')::json, 'name')",
    },
    {
      name: 'ship_to_name',
      sourceAlias: 'ship_to.config',
      dataType: {
        dataType: 'stringType',
      },
      queryable: true,
      visibleByDefault: true,
      valueGetter: "json_extract_path_text((:sourceAlias.jsonb->>'value')::json, 'name')",
