{
  id: '7a14b9b7-701b-4f81-bdb3-c4dee4121fdf',
  name: 'composite_po_instance',
  private: false,
  requiredPermissions: [
    // This inherited from simple_config_data, but is also included here because of the added columns here that also retrieve config data
    "configuration.entries.collection.get",
  ],
  sources: [
    {
      alias: 'pol',
      type: 'entity-type',
      id: '58148257-bfb0-4687-8c42-d2833d772f3e', // simple_purchase_order_line
      useIdColumns: true
    },
    {
      alias: 'purchase_order',
      type: 'entity-type',
      id: '48de4796-11a2-44e9-abb7-01e84af87234', // simple_purchase_order
      join: {
        type: 'RIGHT JOIN',
        joinTo: 'pol.pol',
        condition: ":this.id = (:that.jsonb ->> 'purchaseOrderId')::uuid",
      },
    },
//    {
//      alias: 'location',
//      type: 'entity-type',
//      id: '74ddf1a6-19e0-4d63-baf0-cd2da9a46ca4', // simple_locations
//      join: {
//        type: 'LEFT JOIN',
//        joinTo: 'pol.pol',
//        condition: "jsonb_extract_path_text(:that.jsonb, 'locations', 'locationId')::uuid = :this.id",
//      },
//    },
    {
      alias: 'holdings',
      type: 'entity-type',
      id: '30a5cfad-1868-4f46-86b9-a6ef67e2d9bf', // simple_holdings_records
      essentialOnly: true,
      // Because there could theoretically be multiple holdings associated with a single POL ID, we need to include that ID to avoid ID collisions
      useIdColumns: true,
      join: {
        type: 'LEFT JOIN',
        joinTo: 'pol.pol',
        condition: "EXISTS (SELECT 1 FROM jsonb_array_elements(:that.jsonb -> 'locations') AS hLoc WHERE (hLoc ->> 'holdingId')::uuid = :this.id)",
      },
    },
    {
      alias: 'instance',
      type: 'entity-type',
      id: '8fc4a9d2-7ccf-4233-afb8-796911839862', // simple_instance
      essentialOnly: true,
      join: {
        type: 'LEFT JOIN',
        joinTo: 'pol.pol',
        condition: "(:that.jsonb ->> 'instanceId')::uuid = :this.id",
      },
    },
//    {
//      alias: 'holdings_permanent_location',
//      type: 'entity-type',
//      id: '74ddf1a6-19e0-4d63-baf0-cd2da9a46ca4', // simple_locations
