{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "description": "Entity Type Summary",
  "type": "object",
  "properties": {
    "id": {
      "description": "Entity type id",
      "type": "string",
      "format": "UUID"
    },
    "label": {
      "description": "Entity type label",
      "type": "string"
    },
    "missingPermissions": {
      "description": "List of missing permissions",
      "type": "array",
      "items": {
        "type": "string"
      }
    }
  },
  "additionalProperties": false,
  "required": ["id", "label"]
}
